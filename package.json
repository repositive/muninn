{
  "name": "muninn",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "precommit": "npm run lint",
    "start": "node .",
    "startDev": "nodemon --watch src/main --watch package.json -e ts,json -q -x 'rm -fr dist && echo \"\\n============\\nCOMPILING...\\n============\\n\\n\" && tsc --outDir dist || return 0 && node .'",
    "make": "rm -fr dist && tsc",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "nyc --reporter lcov tape $(find dist -name '*.spec.js' ! -name 'index.js') | tap-spec",
    "coverage": "nyc check-coverage --lines 80 --functions 80 --branches 80"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.spec.js",
      "**/*.d.ts"
    ]
  },
  "author": "Repositive Team",
  "license": "MIT",
  "keywords": [
    "muninn",
    "service"
  ],
  "dependencies": {
    "@repositive/iris": "0.4.0",
    "amqplib": "0.5.1",
    "config": "1.26.1"
  },
  "devDependencies": {
    "@repositive/typescript": "1.1.2",
    "@types/amqplib": "0.5.3",
    "@types/config": "0.0.32",
    "@types/node": "8.0.12",
    "@types/proxyquire": "1.3.27",
    "@types/redis": "0.12.32",
    "@types/sinon": "2.3.2",
    "@types/tape": "4.2.30",
    "husky": "0.14.3",
    "nodemon": "1.11.0",
    "nyc": "11.0.3",
    "proxyquire": "1.8.0",
    "redis": "2.6.2",
    "sinon": "2.3.8",
    "tap-spec": "4.1.1",
    "tape": "4.7.0",
    "tslint": "5.5.0",
    "typescript": "2.4.1"
  }
}
